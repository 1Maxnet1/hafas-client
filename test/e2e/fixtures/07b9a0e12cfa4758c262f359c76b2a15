<script>
    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    if (!sessionStorage.getItem("loggedIn")) {

        var defaultRole = getCookie("WCM_mainRole");
        if (defaultRole) {
            document.cookie = "WCM_selectedRole=" + encodeURIComponent(defaultRole) + ";path=/;secure";
        }

        var xhttp = new XMLHttpRequest();
        xhttp.withCredentials = true;
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4) {
                var answerFromIdP = this.responseText;
                if (answerFromIdP) {
                    var title = answerFromIdP.match(/<title>(.*)?<\/title>/i)[1];
                    var hasSAMLResponse = answerFromIdP.search(/<INPUT TYPE="HIDDEN" NAME="SAMLResponse" VALUE=/i);
                    if (hasSAMLResponse === -1) {
                        // do nothing
                    } else {
                        answerFromIdP = answerFromIdP.replace(/(\r\n|\n|\r)/gm, "");
                        var samlResp = answerFromIdP.match(/<INPUT TYPE="HIDDEN" NAME="SAMLResponse" VALUE="(.*)"><INPUT TYPE="HIDDEN" NAME="RelayState" /i)[1];
                        var xhttp2 = new XMLHttpRequest();
                        xhttp2.open("POST", "https://www.sbb.ch/sp-login/SPAcs/metaAlias/spwwwSBB", true);
                        xhttp2.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
                        xhttp2.send("SAMLResponse=" + encodeURIComponent(samlResp));
                        xhttp2.onreadystatechange = function () {
                            if (this.readyState == 4) {
                                sessionStorage.setItem("loggedIn", true);
                            }
                        };
                    }
                }
            }
        };
        xhttp.open("GET", "https://www.swisspass.ch/oevlogin/check-login?Location=https://www.sbb.ch/sp-login/check-login", true);
        xhttp.send();
    }
</script>

    

    

    




    

    
